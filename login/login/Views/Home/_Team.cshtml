@model login.Models.PeopleData

<!-- /Section header -->
<!-- team -->

    <div class="col-sm-18">
        <div  class="team">
            <div class="team-img">            
                <img class="img-responsive" src="~/images/@Model.Image" alt="Empleado" />
                <div class="overlay">
                    <div class="team-social">
                        <a href="#"><i class="fa fa-facebook"></i></a>
                        <a href="#"><i class="fa fa-google-plus"></i></a>
                        <a href="#"><i class="fa fa-twitter"></i></a>
                    </div>
                </div>
            </div>
            <div class="team-content">
                <h3>@Model.Nombre</h3>
                <span>@Model.Cargo</span>
            </div>
        </div>
    </div>




